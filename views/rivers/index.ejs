<%- include('../partials/header') %>

<table id="list">
    <thead>
      <tr>
        <th>Name</th>
        <th>Date<br>Added</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% rivers.forEach(function(r) { %>
        <tr>
          <td><%= r.name %></td>
          <td><%= r.dateAdded %></td>
          <td><a href="/rivers/<%= r._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>

<%- include('../partials/footer') %>